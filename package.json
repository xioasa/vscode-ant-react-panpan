{
	"name": "ant-react-panpan",
	"displayName": "ant-react-panpan",
	"description": "基于antDesign-react的代码块",
	"keywords": ["ant", "react", "ant design"],
	"version": "1.1.2",
	"homepage": "https://github.com/xioasa/vscode-ant-react-panpan",
	"repository": {
		"type": "git",
		"url": "https://github.com/xioasa/vscode-ant-react-panpan.git"
	},
	"icon": "icon/icon.png",
	"publisher": "xiaosa",
	"engines": {
		"vscode": "^1.25.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./extension.js",
	"contributes": {
		"snippets":[
			{
				"language": "javascriptreact",
				"path": "./snippets/layout.json"
			},
			{
				"language": "javascriptreact",
				"path": "./snippets/compoenent.json"
			},
			{
				"language": "javascriptreact",
				"path": "./snippets/function.json"
			},
			{
				"language": "javascriptreact",
				"path": "./snippets/props&var.json"
			},
			{
				"language": "javascriptreact",
				"path": "./snippets/help.json"
			}
		],
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.25.0",
		"eslint": "^5.13.0",
		"glob": "^7.1.4",
		"mocha": "^10.2.0",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.0-next.0"
	}
}
